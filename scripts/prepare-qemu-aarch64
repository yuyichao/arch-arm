#!/bin/bash

. arch-arm-cross-config.sh

[[ -f /proc/sys/fs/binfmt_misc/arch-qemu-aarch64 ]] && exit

echo ":arch-qemu-aarch64:M::\x7f\x45\x4c\x46\x02\x01\x01\x00\x00\x00\x00\x00\x00\x00\x00\x00\x02\x00\xb7:\xff\xff\xff\xff\xff\xff\xff\x00\xff\xff\xff\xff\xff\xff\xff\xff\xfe\xff\xff:${EXEC_DIR}/arch-qemu-aarch64:P" > /proc/sys/fs/binfmt_misc/register
