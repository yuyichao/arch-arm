#!/bin/bash

. arch-arm-cross-config.sh

[[ -f /proc/sys/fs/binfmt_misc/arch-qemu-arm ]] && exit

echo ":arch-qemu-arm:M::\x7fELF\x01\x01\x01\x00\x00\x00\x00\x00\x00\x00\x00\x00\x02\x00\x28\x00:\xff\xff\xff\xff\xff\xff\xff\x00\xff\xff\xff\xff\xff\xff\xff\xff\xfe\xff\xff\xff:${EXEC_DIR}/arch-qemu-arm:P" > /proc/sys/fs/binfmt_misc/register
