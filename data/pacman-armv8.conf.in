#
# /etc/pacman.conf
#
# See the pacman.conf(5) manpage for option and repository directives

#
# GENERAL OPTIONS
#
[options]
RootDir     = /usr/lib/arch-arm-cross/pkg_root/armv8/
DBPath      = /usr/lib/arch-arm-cross/pkg_root/armv8/var/lib/pacman/
CacheDir    = /usr/lib/arch-arm-cross/pkg_root/armv8/var/cache/pacman/pkg/
LogFile     = /usr/lib/arch-arm-cross/pkg_root/armv8/var/log/pacman.log
GPGDir      = /usr/lib/arch-arm-cross/pkg_root/armv8/etc/pacman.d/gnupg/
Architecture = aarch64
Include     = @CMAKE_INSTALL_FULL_DATADIR@/arch-arm-cross/pacman-arm-options.conf

[core]
Include = @CMAKE_INSTALL_FULL_SYSCONFDIR@/pacman.d/mirrorlist-arm-cross

[extra]
Include = @CMAKE_INSTALL_FULL_SYSCONFDIR@/pacman.d/mirrorlist-arm-cross

[community]
Include = @CMAKE_INSTALL_FULL_SYSCONFDIR@/pacman.d/mirrorlist-arm-cross
